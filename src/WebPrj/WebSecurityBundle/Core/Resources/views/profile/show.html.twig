{% extends 'base.html.twig' %}

{% block body %}
    <h1>Profile</h1>

    <table>
        <tbody>
            <tr>
                <th>Profileid</th>
                <td>{{ profile.profileId }}</td>
            </tr>
            <tr>
                <th>Descprofile</th>
                <td>{{ profile.descProfile }}</td>
            </tr>
            <tr>
                <th>Appid</th>
                <td>{{ profile.appId }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('ums_sec_roles_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('ums_sec_roles_edit', { 'id': profile.profileId }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
